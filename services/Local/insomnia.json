{
  "info": {
    "name": "EV Route Planner API",
    "description": "Collection for testing the Integrated Hybrid EV Route Planner API",
    "version": "3.0.0"
  },
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "description": "Base URL for the API"
    }
  ],
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        },
        "description": "Check API health and configuration status"
      }
    },
    {
      "name": "Get All Stations",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/stations",
          "host": ["{{base_url}}"],
          "path": ["stations"]
        },
        "description": "Get list of all available charging stations"
      }
    },
    {
      "name": "Route Capabilities",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/route-capabilities",
          "host": ["{{base_url}}"],
          "path": ["route-capabilities"]
        },
        "description": "Get information about API routing capabilities"
      }
    },
    {
      "name": "Basic Route Planning",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"source_lat\": 6.9271,\n  \"source_long\": 79.8612,\n  \"destination_lat\": 7.2906,\n  \"destination_long\": 80.6337,\n  \"initial_battery_percent\": 80.0,\n  \"km_per_percent\": 1.4,\n  \"max_charging_stops\": 10,\n  \"min_battery_reserve\": 5.0\n}"
        },
        "url": {
          "raw": "{{base_url}}/ev-route-plan",
          "host": ["{{base_url}}"],
          "path": ["ev-route-plan"]
        },
        "description": "Plan route from Colombo to Kandy with 80% initial battery"
      }
    },
    {
      "name": "Route Planning with Google API",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"source_lat\": 6.9271,\n  \"source_long\": 79.8612,\n  \"destination_lat\": 7.2906,\n  \"destination_long\": 80.6337,\n  \"initial_battery_percent\": 50.0,\n  \"km_per_percent\": 1.4,\n  \"max_charging_stops\": 15,\n  \"min_battery_reserve\": 5.0,\n  \"google_api_key\": \"YOUR_GOOGLE_API_KEY_HERE\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/ev-route-plan",
          "host": ["{{base_url}}"],
          "path": ["ev-route-plan"]
        },
        "description": "Plan route with lower battery requiring more charging stops, using Google API for accurate distances"
      }
    },
